<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>{{viewTitle}}</h1>
    <a href="/cust">Add</a>
    <table border="1">
        <thead>
            <tr>
                <th>Name</th>
                <th>State</th>
                <th>City</th>
                <th>Gender</th>
                <th>Email</th>
                {{!-- <th>Action</th> --}}
            </tr>
        </thead>
        <tbody>
            {{#each list}}
            <tr>
                <td>{{this.name}}</td>
                <td>{{this.state}}</td>
                <td>{{this.city}}</td>
                <td>{{this.gender}}</td>
                <td>{{this.email}}</td>
                <td><a href="/cust/edit/{{this._id}}">Edit</a></td>
                <td><a href="/cust/delete/{{this._id}}">Delete</a></td> 
            </tr>
            {{/each}}
        </tbody>
    </table>
</body>
</html>